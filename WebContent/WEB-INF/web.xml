<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>BU Club Central</display-name>
  <welcome-file-list>
    <welcome-file>Home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>AdminHomeServlet</servlet-name>
    <servlet-class>servlets.AdminHomeServlet</servlet-class>
    <init-param>
      <param-name>AdminHomeServlet</param-name>
      <param-value>Spring</param-value>
    </init-param>
    <security-role-ref>
      <role-name>admin</role-name>
      <role-link>admin</role-link>
    </security-role-ref>
  </servlet>
  <security-role>
    <role-name>admin</role-name>
  </security-role>
  <login-config>
    <auth-method>BASIC</auth-method>
  </login-config>
  <servlet>
    <description></description>
    <display-name>BenUnion</display-name>
    <servlet-name>BenUnion</servlet-name>
    <jsp-file>/WEB-INF/jsp/BenUnion.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>BenUnion</servlet-name>
    <url-pattern>/BenUnion</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>401</error-code>
    <location>/WEB-INF/jsp/ErrorCatchAllPage.jsp</location>
  </error-page>
  <error-page>
    <error-code>403</error-code>
    <location>/WEB-INF/jsp/ErrorCatchAllPage.jsp</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/WEB-INF/jsp/ErrorCatchAllPage.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/WEB-INF/jsp/ErrorCatchAllPage.jsp</location>
  </error-page>
  <error-page>
    <error-code>503</error-code>
    <location>/WEB-INF/jsp/ErrorCatchAllPage.jsp</location>
  </error-page>
</web-app>