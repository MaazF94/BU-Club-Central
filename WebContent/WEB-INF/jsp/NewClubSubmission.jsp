<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<link rel="stylesheet" href="css/NewClubSubmission.css">
<title>New Club Submission</title>
</head>
<body>
    <h1 align=center> <font face=Helvetica color=#CC6666>Student Organization Registration Form </font> </h1>
    <p align=center> <font size=4 face=Helvetica color=#CC6666> Office of Student Engagement and Leadership Development </font></p>
  <input name="name of organization" type="text" class="feedback-input" placeholder="Name of Organization" />
    <input name="Advisor" type="text" class="feedback-input" placeholder="Advisor Full Name" />
    <input name="AdvisorEmail" type="text" class="feedback-input" placeholder="Advisor Email" />
    <input name="AdvisorPhone" type="text" class="feedback-input" placeholder="Advisor Phone Number" /><br>   
  <input name="President" type="text" class="feedback-input" placeholder="President Full Name" />
    <input name="PresidentEmail" type="text" class="feedback-input" placeholder="President Email" />
    <input name="PresidentPhone" type="text" class="feedback-input" placeholder="President Phone Number" />
    <input name="PresidentYearInSchool" type="text" class="feedback-input" placeholder="President Year in School" /><br>

  <input name="VP" type="text" class="feedback-input" placeholder="Vice President Full Name" />
    <input name="VPEmail" type="text" class="feedback-input" placeholder="Vice President Email" />
    <input name="VPPhone" type="text" class="feedback-input" placeholder="Vice President Phone Number" />
    <input name="VPYearInSchool" type="text" class="feedback-input" placeholder="Vice President Year in School" /><br>
    
  <input name="Secretary" type="text" class="feedback-input" placeholder="Secretary Full Name" />
    <input name="SecretaryEmail" type="text" class="feedback-input" placeholder="Secretary Email" />
    <input name="SecretaryPhone" type="text" class="feedback-input" placeholder="Secretary Phone Number" />
    <input name="SecretaryYearInSchool" type="text" class="feedback-input" placeholder="Secretary Year in School" /><br>
    
  <input name="Treasurer" type="text" class="feedback-input" placeholder="Treasurer Full Name" />
    <input name="TreasurerEmail" type="text" class="feedback-input" placeholder="Treasurer Email" />
    <input name="TreasurerPhone" type="text" class="feedback-input" placeholder="Treasurer Phone Number" />
    <input name="TreasurerYearInSchool" type="text" class="feedback-input" placeholder="Treasurer Year in School" /><br>
  
    <input name="DayOfWeek" type="text" class="feedback-input" placeholder="Meeting Day of the Week" />
    <input name="Time" type="text" class="feedback-input" placeholder="Meeting Time" />
    <input name="Place" type="text" class="feedback-input" placeholder="Meeting Place" />
    <input name="HowOften" type="text" class="feedback-input" placeholder="Meeting How Often (Monthly?)" /><br>
  
  <input type="submit" value="SUBMIT"/>  
</body>
</html>